{
  "manifest_version": 3,
  "name": "Tab Tree",
  "version": "1.0.0",
  "description": "Visualize your browser tabs as a tree graph",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Tab Tree"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
