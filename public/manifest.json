{
  "manifest_version": 3,
  "name": "Tab Graph",
  "version": "1.0.0",
  "description": "Visualize your browser tabs as a tree graph",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Tab Graph"
  },
  "icons": {
    "16": "vite.svg",
    "48": "vite.svg",
    "128": "vite.svg"
  }
}
