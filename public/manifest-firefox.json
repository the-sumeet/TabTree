{
  "manifest_version": 2,
  "name": "Tab Tree",
  "version": "1.0.0",
  "description": "Visualize your browser tabs as a tree graph",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Tab Tree"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "tabtree@example.com",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}
